{
  "name": "pos-admin-multi-tenant",
  "version": "1.0.0",
  "description": "Secure, scalable, multi-tenant admin panel for POS and Warehouse Management SaaS",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:unified": "npm run build:frontend && npm run dev:backend:unified",
    "dev:backend:unified": "cross-env NODE_ENV=production nodemon --exec ts-node src/backend/index.ts",
    "build": "node scripts/build.js",
    "build:backend": "tsc",
    "build:frontend": "cd frontend && npm run build",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "start:dev": "cross-env NODE_ENV=development node dist/index.js",
    "start:dev:smart": "node scripts/start-dev.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:migrate": "ts-node src/backend/database/migrate.ts",
    "db:seed": "ts-node src/backend/database/seed.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "multi-tenant",
    "admin-panel",
    "pos",
    "warehouse",
    "saas",
    "typescript",
    "nodejs",
    "react"
  ],
  "author": "Your Company",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1490.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-oauth2": "^1.7.0",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.7.4",
    "speakeasy": "^2.0.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.14.202",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.8.10",
    "@types/nodemailer": "^6.4.14",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-oauth2": "^1.4.38",
    "@types/pg": "^8.10.9",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "concurrently": "^8.2.2",
    "cross-env": "^10.0.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
